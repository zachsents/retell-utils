{
  "name": "retell-utils",
  "version": "0.4.1",
  "description": "Type-safe Zod schemas for Retell AI API resources with lifecycle-aware discriminated unions and generic customization",
  "type": "module",
  "zshy": {
    "exports": "./src/index.ts",
    "cjs": false
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun fix && zshy",
    "fix": "oxlint --fix-suggestions --type-aware && prettier --write .",
    "check": "tsc --noEmit && bun run fix",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "libphonenumber-js": "^1.12.36",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxlint": "latest",
    "oxlint-tsgolint": "latest",
    "prettier": "latest",
    "prettier-plugin-jsdoc": "^1.8.0",
    "typescript": "^5.8.3",
    "zshy": "^0.7.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachsents/retell-utils"
  },
  "keywords": [
    "retell",
    "retell-ai",
    "zod",
    "schemas",
    "voice-agent",
    "type-safety"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
}
